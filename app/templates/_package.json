{
  "name": "<%= projectName %>",
  "version": "0.0.0",
  "description": "<%= projectDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= projectName %>",
  "scripts": {
    "start": "npm run dev:server",
    "dev:server": "budo src/index.js --title <%= projectName %> --host -P -o -l static -- -t sheetify/transform -g es2040",
    "build:prod": "scripts/build-prod.sh",
    "lint": "standard --verbose | snazzy",
    "test": "npm run lint && ava --verbose"
  },
  "dependencies": {
    "choo": "^3.0.2"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "budo": "8.3.0",
    "es2040": "1.2.2",
    "envify": "^3.4.1",
    "normalize.css": "^4.1.1",
    "sheetify": "^5.0.3",
    "standard": "^7.1.2",
    "snazzy": "^4.0.0",
    "uglifyify": "^3.0.2",
    "unassertify": "^2.0.3",
    "yo-yoify": "^3.3.0"
  },
  "standard": {
    "ignore": [
      "scripts"
    ]
  }
}
