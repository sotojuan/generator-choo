    "budo": "8.3.0",
    "es2040": "1.2.2",
    "eslint": "3.1.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "envify": "^3.4.1",
    "normalize.css": "^4.1.1",
    "sheetify": "^5.0.3",
    "uglifyify": "^3.0.2",
    "unassertify": "^2.0.3",
    "yo-yoify": "^3.3.0"
  }
